<html lang='es'>
  <head>
    {{>head}}
  </head>
  <body>
    <div class="title">
      {{{title}}}
    </div>
    <br>
      <div class="container-md" style="width: 100vh">
        <table class="table align-content-center">
        <tbody id="tbodyProdTest">
          <tr>
            <td>Modo: </td>
            <td>{{config.modo}} </td>
          </tr>
          <tr>
            <td>Persistencia: </td>
            <td>{{config.persistencia}} </td>
          </tr>
          <tr>
            <td>PID: </td>
            <td>{{PID}} </td>
          </tr>
          <tr>
            <td>Version: </td>
            <td>{{version}} </td>
          </tr>
          <tr>
            <td>Carpeta: </td>
            <td>{{carpeta}} </td>
          </tr>
          <tr>
            <td>Sistema : </td>
            <td>{{OS}} </td>
          </tr>
          <tr>
            <td>Memoria total reservada (rss) : </td>
            <td>{{memoryUsage}} </td>
          </tr>
        </tbody>
        </table>
      </div>
  </body>
  {{> footer}}
</html>



